{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Next.js: Dev Server",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "internalConsoleOptions": "neverOpen"
        }
    ]
}